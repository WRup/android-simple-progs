<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wrup.lab04">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Students"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.wrup.lab04.StudentListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.wrup.lab04.StudentGroupsActivity"
            android:windowSoftInputMode="adjustNothing" />
        <activity android:name="com.example.wrup.lab04.GroupListActivity"></activity>
    </application>

</manifest>